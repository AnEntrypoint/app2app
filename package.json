{
  "name": "pupdebug",
  "version": "1.0.0",
  "description": "CLI tool for automated debugging",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/cli.js",
    "lint": "eslint . --ext .js",
    "test": "jest --forceExit --detectOpenHandles --runInBand --testTimeout=10000",
    "test:e2e": "jest --forceExit --detectOpenHandles --runInBand --testTimeout=10000 --testMatch='**/test/e2e/**/*.js'",
    "build": "babel src --out-dir dist"
  },
  "dependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.8",
    "babel-jest": "^27.5.1",
    "commander": "^8.3.0",
    "dotenv": "^10.0.0",
    "ignore": "^5.3.2",
    "jest": "^27.5.1",
    "jest-junit": "^12.3.0",
    "node-fetch": "^2.7.0",
    "puppeteer": "^13.7.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.31.0"
  }
}
