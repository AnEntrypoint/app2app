{
  "name": "pupdebug",
  "version": "1.0.1",
  "description": "CLI tool for automated debugging",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/cli.js",
    "lint": "eslint . --ext .js",
    "test": "npm run lint; jest --forceExit --detectOpenHandles --runInBand --testTimeout=10000",
    "test:e2e": "jest --forceExit --detectOpenHandles --runInBand --testTimeout=10000 --testMatch='**/test/e2e/**/*.js'",
    "build": "babel src --out-dir dist",
    "release": "git commit -a -m ranrelease && npm version patch  && npm publish --tag latest"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "puppeteer": "^24.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.8",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "ignore": "^7.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "node-fetch": "^3.3.2"
  }
}
