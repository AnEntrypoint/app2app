{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-control-regex": "off",
    "no-useless-escape": "off",
    "no-undef": "off",
    "no-unused-vars": "warn"
  },
  "ignorePatterns": [
    "dist/**",
    "*.config.js",
    "**/*.test.js"
  ]
}